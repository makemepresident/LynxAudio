CREATE TABLE IF NOT EXISTS audio_clips (
    id SERIAL,
    userid INTEGER,
    filename VARCHAR(255)
    cliplength INTEGER
    filesize INTEGER,
    url_hash VARCHAR(10),
    PRIMARY KEY (ID) SERIAL
);

CREATE TABLE IF NOT EXISTS users (
    id SERIAL,
    username VARCHAR(30),
    password VARCHAR(255),
    first VARCHAR(30),
    last VARCHAR(30),
    email VARCHAR(50),
    PRIMARY KEY (ID) SERIAL
);