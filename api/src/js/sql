CREATE TABLE IF NOT EXISTS audio_clips (
    id SERIAL,
    userid INTEGER,
    audiobinary BYTEA,
    cliplength SMALLINT,
    filesize SMALLINT,
    url_hash VARCHAR(10),
    PRIMARY KEY (ID) SERIAL
);

CREATE TABLE IF NOT EXISTS users (
    id INTEGER,
    username CHAR(64),
    "password" VARCHAR(30),
    "first" VARCHAR(12),
    "last" VARCHAR(12),
    email VARCHAR(30),
    icon BYTEA
);